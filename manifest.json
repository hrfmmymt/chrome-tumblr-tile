{
    "name": "tumblr-tile",
    "version": "0.1",
    "description": "tumblr-tile",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "https://api.tumblr.com/"
    ],

    "options_page": "options.html",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://api.tumblr.com; object-src 'self'"
}